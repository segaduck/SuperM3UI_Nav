                    Margin="0,0,0,0"
                    VerticalAlignment="Center"
                    Canvas.ZIndex="1"
                    RadiusX="20"
                    RadiusY="20">
                    <Rectangle.Fill>
                        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                            <!--  第一個顏色為上半部的顏色  -->
                            <GradientStop Offset="0" Color="#30ADD8E6" />
                            <!--  第二個顏色為下半部的顏色  -->
                            <GradientStop Offset="1" Color="#aaADD8E6" />
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>
