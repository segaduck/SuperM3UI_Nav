    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35" />
            <RowDefinition Height="10" />
            <RowDefinition Height="50*" />
            <RowDefinition Height="10" />
        </Grid.RowDefinitions>
        <Grid x:Name="gdSelect" Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="45*" />
                <ColumnDefinition Width="55*" />
            </Grid.ColumnDefinitions>
            <ComboBox
                x:Name="cb_selectInput"
                Grid.Column="0"
                Width="300"
                Height="30"
                Margin="40,0,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                FontFamily="Microsoft JhengHei UI"
                FontSize="18"
                FontWeight="Black"
                SelectionChanged="cb_selectInput_SelectionChanged"
                Style="{StaticResource roundComboBox}" />
            <TextBlock
                Name="outP"
                Grid.Column="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Top"
                FontSize="18"              
                Foreground="White"/>
            <ComboBox
                x:Name="cbSpikeMode"
                Grid.Column="1"
                Width="300"
                Height="30"
                FontFamily="Microsoft JhengHei UI"
                FontSize="18"
                FontWeight="Black"
                VerticalAlignment="Top"
                HorizontalAlignment="Center"
                Margin="30,0,0,0"
                SelectionChanged="cbSpikeMode_SelectionChanged"
                Style="{StaticResource roundComboBox}" />

        </Grid>
